CONFIG_INPUT_MISC_FPC1020=m
CONFIG_INPUT_MISC_FPC1020_SAVE_TO_CLASS_DEVICE=y
CONFIG_INPUT_NOVA_DRM_PANEL=y
CONFIG_INPUT_QPNP_POWER_ON_MMI=m
CONFIG_INPUT_TOUCHSCREEN_HIMAX_V3_MMI_0FLASH=y
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI=m
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI_ESD=y
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI_GESTURE=y
CONFIG_NOVA_ESD_ENABLE=y
