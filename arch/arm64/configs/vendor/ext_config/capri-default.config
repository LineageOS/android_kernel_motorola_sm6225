CONFIG_INPUT_NOVA_CHIP_VER_1=y
CONFIG_INPUT_NOVA_DRM_PANEL=y
CONFIG_INPUT_QPNP_POWER_ON_MMI=m
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI=m
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI_FW_PANEL=y
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI_PANEL_NOTIFICATIONS=y
CONFIG_INPUT_TOUCHSCREEN_ILI_9882_MMI_RESUME_BY_DDI=y
CONFIG_NOVA_ESD_ENABLE=y
CONFIG_NOVA_LCM_FAST_LIGHTUP=y
CONFIG_SARSENSOR_SX933X=m
