CONFIG_DTS201A_MMI=m
CONFIG_INPUT_CHIPONE_FPS_MMI=m
CONFIG_INPUT_FOCALTECH_0FLASH_MMI_IC_NAME_FT8006S_AA=y
CONFIG_INPUT_MISC_FPC1020=m
CONFIG_INPUT_MISC_FPC1020_SAVE_TO_CLASS_DEVICE=y
CONFIG_INPUT_NOVA_DRM_PANEL=y
CONFIG_KTD3136_BL=m
CONFIG_LEDS_AW99703=m
CONFIG_NOVA_ESD_ENABLE=y
CONFIG_TOUCHSCREEN_CHIPONE_TDDI=m
CONFIG_TOUCHSCREEN_CHIPONE_TDDI_ESD=y
CONFIG_INPUT_TOUCHSCREEN_FOCALTECH_0FLASH_MMI=m
CONFIG_INPUT_TOUCHSCREEN_FOCALTECH_0FLASH_MMI_DRM_PANEL=y
CONFIG_TOUCHSCREEN_NT36xxx_HOSTDL_SPI_MMI=m
