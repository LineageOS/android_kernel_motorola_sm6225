&qupv3_se0_i2c {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	pinctrl-0 = <&qupv3_se0_i2c_act>;
	pinctrl-1 = <&qupv3_se0_i2c_sle>;
	wt6670: wt6670f@35 {
		status = "okay";
		compatible = "mediatek,wt6670f_qc3p";
		reg = <0x35>;
		#io-channel-cells = <1>;
		wt6670f-reset-gpio = <&tlmm 68 0x0>;
		wt6670f-int-gpio = <&tlmm 93 0x0>;
	};
};

&qupv3_se0_i2c {
	status = "okay";

	cw2217@64 {
