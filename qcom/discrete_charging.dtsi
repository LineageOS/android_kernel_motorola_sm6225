		#include "bat_setting/CW_ng50-SWD-5000mah-rhode.dtsi"
		#include "bat_setting/CW_ng50-BA-ATL-5000mah-rhode.dtsi"
		#include "bat_setting/CW_ng50-IN-ATL-5000mah-rhode.dtsi"
	};

	sm5602@71 {
		compatible = "sm,sm5602";
		reg = <0x71>;
